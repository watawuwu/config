<?xml version="1.0"?>
<root>
  <devicevendordef>
    <vendorname>TOPRE</vendorname>
    <vendorid>0x0853</vendorid>
  </devicevendordef>
  <deviceproductdef>
    <productname>REAL_FORCE</productname>
    <productid>0x0104</productid>
  </deviceproductdef>
  <appdef>
    <appname>INTELIJ</appname>
    <equal>com.jetbrains.intellij</equal>
    <equal>com.jetbrains.intellij.ce</equal>
    <equal>com.jetbrains.WebStorm</equal>
  </appdef>

  <replacementdef>
    <replacementname>EMACS_MODE_IGNORE_APPS</replacementname>
    <replacementvalue>
      ECLIPSE,
      EMACS,
      REMOTEDESKTOPCONNECTION,
      TERMINAL,
      VI,
      VIRTUALMACHINE,
      X11,
      INTELIJ,
      SCREEN_SHARING
    </replacementvalue>
  </replacementdef>
    <item>
        <name>For Google Chrome</name>
        <item>
            <name>Control B to History Back</name>
            <identifier>private.orz.fuscus.change_ctl_b_to_history_back</identifier>
            <only>GOOGLE_CHROME</only>
            <autogen>--KeyToKey-- KeyCode::B,ModifierFlag::COMMAND_L, KeyCode::CURSOR_LEFT, ModifierFlag::COMMAND_L</autogen>
        </item>
        <item>
            <name>Control F to History Forward</name>
            <identifier>private.orz.fuscus.change_ctl_f_to_history_forward</identifier>
            <only>GOOGLE_CHROME</only>
            <autogen>--KeyToKey-- KeyCode::F, ModifierFlag::COMMAND_L,KeyCode::CURSOR_RIGHT, ModifierFlag::COMMAND_L</autogen>
        </item>
        <item>
            <name>Control , to Select Next Tab</name>
            <identifier>private.orz.fuscus.change_ctrl_dot_to_command_option_right</identifier>
            <only>GOOGLE_CHROME</only>
            <autogen>--KeyToKey-- KeyCode::DOT, ModifierFlag::CONTROL_L,KeyCode::CURSOR_RIGHT, ModifierFlag::COMMAND_L | ModifierFlag::OPTION_L</autogen>
        </item>
        <item>
            <name>Control , to Select Prev Tab</name>
            <identifier>private.orz.fuscus.change_ctrl_comma_to_command_option_prev</identifier>
            <only>GOOGLE_CHROME</only>
            <autogen>--KeyToKey-- KeyCode::COMMA, ModifierFlag::CONTROL_L,KeyCode::CURSOR_LEFT, ModifierFlag::COMMAND_L | ModifierFlag::OPTION_L</autogen>
        </item>
    </item>
    <item>
        <name>Emacs mode Custom</name>
        <item>
            <name>Option H to delete back word</name>
            <identifier>private.orz.fuscus.change_option_h_to_option_delete</identifier>
            <autogen>--KeyToKey-- KeyCode::H, ModifierFlag::OPTION_L,KeyCode::DELETE, ModifierFlag::OPTION_L</autogen>
        </item>
    </item>

     <item>
        <name>For Intellij IDEA</name>
        <item>
            <name>Escape</name>
            <identifier>private.orz.fuscus.change_ctl_g_to_escape</identifier>
            <only>INTELIJ</only>
            <autogen>--KeyToKey-- KeyCode::G,ModifierFlag::CONTROL_L, KeyCode::ESCAPE</autogen>
        </item>
        <item>
            <name>Move down</name>
            <identifier>private.orz.fuscus.change_ctl_n_to_down</identifier>
            <only>INTELIJ</only>
            <autogen>--KeyToKey-- KeyCode::N,ModifierFlag::CONTROL_L, KeyCode::CURSOR_DOWN</autogen>
        </item>
        <item>
            <name>Move up</name>
            <identifier>private.orz.fuscus.change_ctl_p_to_up</identifier>
            <only>INTELIJ</only>
            <autogen>--KeyToKey-- KeyCode::P,ModifierFlag::CONTROL_L, KeyCode::CURSOR_UP</autogen>
        </item>
        <item>
            <name>Enter</name>
            <identifier>private.orz.fuscus.change_ctl_m_to_enter</identifier>
            <only>INTELIJ</only>
            <autogen>--KeyToKey-- KeyCode::M,ModifierFlag::CONTROL_L, KeyCode::RETURN</autogen>
        </item>
    </item>
</root>

